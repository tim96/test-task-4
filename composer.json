{
    "name": "root/app",
    "description": "Test task 4",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "autoload-dev": {
            "psr-4": {
                "App\\Tests\\": "tests/"
            }
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "guzzlehttp/guzzle": "^7.4",
        "moneyphp/money": "^3.3",
        "symfony/config": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "symfony": {
            "require": "4.4.*"
        }
    }
}
